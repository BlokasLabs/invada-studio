<http://invadarecords.com/plugins/lv2/filter/gui>
    a uiext:GtkUI;
    uiext:binary <inv_filter_gui.so>;
    uiext:requiredFeature uiext:makeResident ;

    uiext:portNotification [ 
	uiext:plugin <http://invadarecords.com/plugins/lv2/filter/lpf/mono> ;
        uiext:portIndex 3 ;
    ],[ 
	uiext:plugin <http://invadarecords.com/plugins/lv2/filter/lpf/mono> ;
        uiext:portIndex 4 ;
    ] ;

    uiext:portNotification [ 
	uiext:plugin <http://invadarecords.com/plugins/lv2/filter/hpf/mono> ;
        uiext:portIndex 3 ;
    ],[ 
	uiext:plugin <http://invadarecords.com/plugins/lv2/filter/hpf/mono> ;
        uiext:portIndex 4 ;
    ] ;

    uiext:portNotification [ 
	uiext:plugin <http://invadarecords.com/plugins/lv2/filter/lpf/stereo> ;
        uiext:portIndex 3 ;
    ],[ 
	uiext:plugin <http://invadarecords.com/plugins/lv2/filter/lpf/stereo> ;
        uiext:portIndex 4 ;
    ],[ 
	uiext:plugin <http://invadarecords.com/plugins/lv2/filter/lpf/stereo> ;
        uiext:portIndex 7 ;
    ],[ 
	uiext:plugin <http://invadarecords.com/plugins/lv2/filter/lpf/stereo> ;
        uiext:portIndex 8 ;
    ] ;


    uiext:portNotification [ 
	uiext:plugin <http://invadarecords.com/plugins/lv2/filter/hpf/stereo> ;
        uiext:portIndex 3 ;
    ],[ 
	uiext:plugin <http://invadarecords.com/plugins/lv2/filter/hpf/stereo> ;
        uiext:portIndex 4 ;
    ],[ 
	uiext:plugin <http://invadarecords.com/plugins/lv2/filter/hpf/stereo> ;
        uiext:portIndex 7 ;
    ],[ 
	uiext:plugin <http://invadarecords.com/plugins/lv2/filter/hpf/stereo> ;
        uiext:portIndex 8 ;
    ] .


<http://invadarecords.com/plugins/lv2/filter/lpf/mono> 
    a lv2:Plugin, lv2:LowpassPlugin ;
    doap:name "Invada Mono LPF" ;
    doap:maintainer [
	foaf:name "Fraser Stuart";
	foaf:homepage <http://www.invadarecords.com/Downloads.php?ID=---TBA---> ;
	foaf:mbox <mailto:fraser@arkhostings.com> ;
    ] ;
    doap:license <http://usefulinc.com/doap/licenses/gpl> ;
    lv2:optionalFeature lv2:hardRtCapable ;
    uiext:ui <http://invadarecords.com/plugins/lv2/filter/gui>;

    lv2:port [
	a lv2:InputPort, lv2:ControlPort ;
	lv2:index 0 ;
	lv2:symbol "freq" ;
	lv2:name "Frequency" ;
	lv2:default 0.015811388 ;
	lv2:minimum 0.0005 ;
	lv2:maximum 0.5 ;
	lv2:portProperty :sampleRate;
	units:unit :hz ;
    ] , [
	a lv2:InputPort, lv2:ControlPort ;
	lv2:index 1 ;
	lv2:symbol "gain" ;
	lv2:name "Gain" ;
	lv2:default 0.0 ;
	lv2:minimum 0.0 ;
	lv2:maximum 12.0 ;
	units:unit :db ;
    ] , [
	a lv2:InputPort, lv2:ControlPort ;
	lv2:index 2 ;
	lv2:symbol "noClip" ;
	lv2:name "Soft Clip" ;
	lv2:portProperty :toggled ;
	lv2:default 0.0 ;
    ] , [
	a lv2:OutputPort, lv2:ControlPort ;
	lv2:index 3 ;
	lv2:symbol "meterIn" ;
	lv2:name "In" ;
	lv2:minimum -60.0 ;
	lv2:maximum 6.0 ;
	units:unit :db ;
    ] , [
	a lv2:OutputPort, lv2:ControlPort ;
	lv2:index 4 ;
	lv2:symbol "meterOut" ;
	lv2:name "Out" ;
	lv2:minimum -60.0 ;
	lv2:maximum 6.0 ;
	units:unit :db ;
    ] , [
	a lv2:InputPort, lv2:AudioPort ;
	lv2:index 5 ;
	lv2:symbol "in" ;
	lv2:name "In" ;
    ] , [
	a lv2:OutputPort, lv2:AudioPort ;
	lv2:index 6 ;
	lv2:symbol "out" ;
	lv2:name "Out" ;
    ]
.

