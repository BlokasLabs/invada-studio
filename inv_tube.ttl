@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://invadarecords.com/plugins/inv_tube_mono> a lv2:Plugin ;
    a lv2:DistortionPlugin ;
    doap:name "Invada Mono Tube Distortion" ;
    doap:maintainer [
	foaf:name "Fraser Stuart";
	foaf:homepage <http://www.invadarecords.com/Downloads.php?ID=---TBA---> ;
	foaf:mbox <mailto:fraser@arkhostings.com> ;
    ] ;
    doap:licence <http://usefulinc.com/doap/licenses/gpl> ;
    lv2:optionalFeature lv2:hardRtCapable ;

    lv2:port [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 0 ;
	lv2:symbol "drive" ;
	lv2:name "Drive" ;
	lv2:default 0.0 ;
	lv2:minimum 0.0 ;
	lv2:maximum 18.0 ;
	lv2:scalePoint [
	    rdfs:label "0dB" ;
	    rdf:value 0.0 ;
	] , [
	    rdfs:label "+6dB" ;
	    rdf:value 6.0 ;
	] , [
	    rdfs:label "+12dB" ;
	    rdf:value 12.0 ;
	] , [
	    rdfs:label "+18dB" ;
	    rdf:value 18.0 ;
	]
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 1 ;
	lv2:symbol "dcoffset" ;
	lv2:name "DC Offset" ;
	lv2:default 0.0 ;
	lv2:minimum -1.0 ;
	lv2:maximum 1.0 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 2 ;
	lv2:symbol "phase" ;
	lv2:name "Ø" ;
	lv2:toggled ;
	lv2:default 0.0 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 3 ;
	lv2:symbol "mix" ;
	lv2:name "Mix" ;
	lv2:default 75.0 ;
	lv2:minimum 0.0 ;
	lv2:maximum 100.0 ;
	lv2:scalePoint [
	    rdfs:label "Dry" ;
	    rdf:value 0.0 ;
	] , [
	    rdfs:label "25%" ;
	    rdf:value 25.0 ;
	] , [
	    rdfs:label "50%" ;
	    rdf:value 50.0 ;
	] , [
	    rdfs:label "75%" ;
	    rdf:value 75.0 ;
	] , [
	    rdfs:label "Wet" ;
	    rdf:value 100.0 ;
	]
    ] , [
	a lv2:AudioPort ;
	a lv2:InputPort ;
	lv2:index 4 ;
	lv2:symbol "in" ;
	lv2:name "In" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:OutputPort ;
	lv2:index 5 ;
	lv2:symbol "out" ;
	lv2:name "Out" ;
    ]
.

